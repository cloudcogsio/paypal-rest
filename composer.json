{
  "name": "cloudcogsio/paypal-rest",
  "type": "library",
  "license": "MIT",
  "authors": [
    {"name": "Ricardo Assing", "email": "ricardo@tsiana.ca"}
  ],
  "keywords": [
    "cloudcogs","cloudcogs.io","omnipay","PayPal","REST","api"
  ],
  "description": "PHP SDK for PayPal's REST API with Omnipay compatible Gateway",
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "omnipay/common": "^3",
    "ext-json": "*",
    "ramsey/uuid": "^4.2.3",
    "symfony/http-client": "6.3.x-dev",
    "nyholm/psr7": "^1.8@dev",
    "php-http/httplug": "2.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Cloudcogs\\PayPal\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
